  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n                da.id,\n                da.event_id,\n                da.attempt_number,\n                da.http_status_code,\n                da.success,\n                da.duration_ms,\n                da.attempted_at,\n                da.error_message,\n                LEFT(da.response_body, 1000) as response_body\n            FROM delivery_attempts da\n            WHERE da.endpoint_id = $1 AND da.success = $2\n            ORDER BY da.attempted_at DESC\n            LIMIT $3 OFFSET $4\n            ",
